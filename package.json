{
  "name": "rn-whitelabel-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint . --ext .ts,.tsx",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "prebuild": "expo prebuild",
    "prebuild:clean": "expo prebuild --clean",
    "build:default": "BRAND=default expo prebuild --clean",
    "build:brand-a": "BRAND=brand-a expo prebuild --clean",
    "build:dev": "APP_ENV=development expo prebuild --clean",
    "build:staging": "APP_ENV=staging expo prebuild --clean",
    "build:prod": "APP_ENV=production expo prebuild --clean",
    "build:ios:default": "BRAND=default expo run:ios",
    "build:ios:brand-a": "BRAND=brand-a expo run:ios",
    "build:android:default": "BRAND=default expo run:android",
    "build:android:brand-a": "BRAND=brand-a expo run:android",
    "fastlane:ios:build": "cd fastlane && bundle exec fastlane ios build",
    "fastlane:ios:beta": "cd fastlane && bundle exec fastlane ios beta",
    "fastlane:ios:release": "cd fastlane && bundle exec fastlane ios release",
    "fastlane:android:build": "cd fastlane && bundle exec fastlane android build",
    "fastlane:android:beta": "cd fastlane && bundle exec fastlane android beta",
    "fastlane:android:release": "cd fastlane && bundle exec fastlane android release",
    "validate:brands": "node scripts/validate-brands.js"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.1.2",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-navigation/bottom-tabs": "^7.10.1",
    "@react-navigation/native": "^7.1.28",
    "@react-navigation/native-stack": "^7.10.1",
    "@reduxjs/toolkit": "^2.11.2",
    "@sentry/react-native": "^7.10.0",
    "axios": "^1.13.2",
    "expo": "~54.0.32",
    "expo-constants": "~18.0.13",
    "expo-secure-store": "~15.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "i18next": "^25.8.0",
    "react": "19.1.0",
    "react-i18next": "^16.5.3",
    "react-native": "0.81.5",
    "react-native-safe-area-context": "^5.6.2",
    "react-native-screens": "^4.20.0",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/react-native": "^13.3.3",
    "@types/react": "~18.3.12",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-native": "^5.0.0",
    "jest": "^30.2.0",
    "patch-package": "^8.0.1",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^3.8.1",
    "typescript": "~5.3.3"
  },
  "private": true
}
